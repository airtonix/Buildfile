{
  "compilerOptions": {
    "lib": ["es2015"],
    "target": "es3",
    "moduleResolution": "node",
    "noImplicitAny": true,
    "strict": true,
    "skipLibCheck": true,
    "noUnusedLocals": true,
    "esModuleInterop": true,
    "plugins": [{
      "name": "typescript-tslint-plugin",
      "suppressWhileTypeErrorsPresent": true
    }]
  }
}
